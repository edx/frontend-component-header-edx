{
  "name": "@edx/frontend-component-header-edx",
  "version": "1.0.0-semantically-released",
  "description": "The standard header for edX",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "make build",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage"
  },
  "files": [
    "/dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-component-header-edx.git"
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/edx/frontend-component-header-edx/issues"
  },
  "homepage": "https://github.com/edx/frontend-component-header-edx#readme",
  "devDependencies": {
    "@edx/brand": "npm:@edx/brand-edx.org@2.0.3",
    "@edx/frontend-build": "5.4.0",
    "@edx/frontend-platform": "1.12.7",
    "@edx/paragon": "12.6.1",
    "codecov": "3.8.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "husky": "7.0.2",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.5",
    "react-router-dom": "5.3.0",
    "react-test-renderer": "16.14.0",
    "reactifex": "1.1.1",
    "redux": "4.1.1",
    "redux-saga": "1.1.3"
  },
  "dependencies": {
    "@edx/frontend-enterprise": "4.6.3",
    "babel-polyfill": "6.26.0",
    "react-responsive": "8.2.0",
    "react-transition-group": "4.4.2"
  },
  "peerDependencies": {
    "@edx/frontend-platform": "^1.8.0",
    "@edx/paragon": "^12.4.0",
    "prop-types": "^15.5.10",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
